{
  "name": "desktop-commander",
  "description": "Desktop control and command execution",
  "npm_package": "@wonderwhy-er/desktop-commander",
  "path": "../mcp_servers/node_modules/@wonderwhy-er/desktop-commander",
  "settings": {
    "allowed_commands": [
      "ls",
      "pwd",
      "echo",
      "cat",
      "grep",
      "find"
    ],
    "max_output_length": 10000,
    "timeout": 30000
  }
}